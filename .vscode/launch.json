  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Migration - Fetch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/migrations/run.js",
      "args": ["-f"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Migration - Write",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/migrations/run.js",
      "args": ["-wtu"]
    }
  ]
}